// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserSchema {
    tgId Int @unique
    firstName String
    lastName String
    isBot Boolean
    username String? @unique
    promptTokens Int? 
    completionTokens Int?
    timestamps Boolean?
}




